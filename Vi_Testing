    }
    
    -- Chia văn bản thành các từ và dịch từng từ
    local words = {}
    for word in text:gmatch("%S+") do
        table.insert(words, translations[word:lower()] or word)
    end

    -- Nối các từ đã dịch lại thành một chuỗi
    return table.concat(words, " ")
end

-- Hàm thay thế văn bản trong GUI
function replaceTextInGui(gui)
    if gui:IsA("TextLabel") or gui:IsA("TextButton") then
        gui.Text = translateText(gui.Text)
    end

    for _, child in ipairs(gui:GetChildren()) do
        replaceTextInGui(child)
    end
end

-- Hàm thay thế văn bản trong CoreGui
function replaceTextInCoreGui()
    local coreGui = game:GetService("CoreGui")
    
    for _, gui in ipairs(coreGui:GetChildren()) do
        replaceTextInGui(gui)
    end
end

-- Hàm thay thế văn bản trong PlayerGui của tất cả người chơi
function replaceTextInPlayerGui()
    for _, player in ipairs(game:GetService("Players"):GetPlayers()) do
        local playerGui = player:FindFirstChildOfClass("PlayerGui")
        if playerGui then
            for _, gui in ipairs(playerGui:GetChildren()) do
                replaceTextInGui(gui)
            end
        end
    end
end

local lastCoreGuiText = {}
local lastPlayerGuiText = {}

local function updateGui(gui, lastText)
    local currentText = {}
    for _, child in ipairs(gui:GetChildren()) do
        if child:IsA("TextLabel") or child:IsA("TextButton") then
            currentText[child] = translateText(child.Text)
        end
        updateGui(child, lastText)
    end

    for gui, text in pairs(currentText) do
        if lastText[gui] ~= text then
            gui.Text = text
            lastText[gui] = text
        end
    end
end

local function updateAllGuis()
    updateGui(game:GetService("CoreGui"), lastCoreGuiText)
    replaceTextInPlayerGui()
end

updateAllGuis()

while true do
    updateAllGuis()
    wait(4) 
end
